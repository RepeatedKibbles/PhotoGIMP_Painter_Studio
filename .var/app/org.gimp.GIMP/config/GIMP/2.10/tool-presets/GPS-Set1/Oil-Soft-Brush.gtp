# GIMP tool preset file

(icon-name "gimp-tool-paintbrush")
(name "Oil Soft Brush")
(tool-options "GimpPaintOptions"
    (tool "gimp-paintbrush-tool")
    (brush "O_Oil Soft")
    (dynamics "Oil Soft Brush")
    (gradient "FG to Transparent")
    (palette "Color History")
    (brush-size 80)
    (brush-spacing 0.05)
    (jitter-amount 0.34999999999999998)
    (fade-length 138)
    (fade-unit percent)
    (gradient-reverse yes))
(use-fg-bg no)
(use-opacity-paint-mode yes)
(use-brush yes)
(use-dynamics yes)
(use-mypaint-brush no)
(use-gradient no)
(use-pattern no)
(use-palette no)
(use-font no)

# end of GIMP tool preset file
