# GIMP tool preset file

(icon-name "gimp-tool-paintbrush")
(name "Graphite Shades 02")
(tool-options "GimpPaintOptions"
    (tool "gimp-paintbrush-tool")
    (opacity 0.25)
    (brush "P_Graphite Pencil _grain")
    (dynamics "Graphite Shades 02")
    (gradient "Deep Sea")
    (palette "Color History")
    (brush-size 250)
    (brush-spacing 0.20)
    (application-mode incremental)
    (use-jitter yes)
    (jitter-amount 0)
    (fade-length 138)
    (fade-unit percent))
(use-fg-bg no)
(use-opacity-paint-mode yes)
(use-brush yes)
(use-dynamics yes)
(use-mypaint-brush no)
(use-gradient no)
(use-pattern no)
(use-palette no)
(use-font no)

# end of GIMP tool preset file
